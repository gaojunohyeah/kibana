              },
              "serverId": {
                "type": "string",
                "index": "not_analyzed"
              },
              "newCharge": {
                "type": "long",
                "index": "not_analyzed"
              },
              "totalRechargeStone": {
                "type": "long",
                "index": "not_analyzed"
              },
              "consumeRMB": {
                "type": "long",
                "index": "not_analyzed"
              },
              "consumeMoney": {
                "type": "long",
                "index": "not_analyzed"
              },
              "totalConsumeUser": {
                "type": "long",
                "index": "not_analyzed"
              },
              "totalChargeUser": {
                "type": "long",
                "index": "not_analyzed"
              },
              "totalStone": {
                "type": "long",
                "index": "not_analyzed"
              },
              "totalgmStone": {
                "type": "long",
                "index": "not_analyzed"
              },
              "arpu": {
                "type": "double",
                "index": "not_analyzed"
              }
            }
          }
        }
      },
      "vip_stat": {
        "properties": {
          "message": {
            "properties": {
              "timestamp": {
                "format": "dateOptionalTime",
                "type": "date",
                "index": "not_analyzed"
              },
              "@timestamp": {
                "format": "dateOptionalTime",
                "type": "date",
                "index": "not_analyzed"
              },
              "gameCode": {
                "type": "string",
                "index": "not_analyzed"
              },
              "regionId": {
                "type": "string",
                "index": "not_analyzed"
              },
              "serverId": {
                "type": "string",
                "index": "not_analyzed"
              },
              "vip0": {
                "type": "long",
                "index": "not_analyzed"
              },
              "vip1": {
                "type": "long",
                "index": "not_analyzed"
              },
              "vip2": {
                "type": "long",
                "index": "not_analyzed"
              },
              "vip3": {
                "type": "long",
