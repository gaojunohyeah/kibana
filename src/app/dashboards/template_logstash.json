{
  "template_logstash": {
    "template": "logstash-*",
    "settings": {
      "number_of_shards": 5,
      "number_of_replicas": 1,
      "index.refresh_interval": "5s",
      "index": {
        "store": {
          "compress": {
            "stored": true,
            "tv": true
          }
        }
      }
    },
    "mappings": {
      "_default_": {
        "_all": {
          "enabled": false
        },
        "_source": {
          "compress": true
        },
        "dynamic_templates": [
          {
            "string_fields": {
              "match": "*",
              "match_mapping_type": "string",
              "mapping": {
                "type": "multi_field",
                "fields": {
                  "{name}": {
                    "type": "string",
                    "index": "analyzed",
                    "omit_norms": true
                  },
                  "raw": {
                    "type": "string",
                    "index": "not_analyzed",
                    "ignore_above": 256
                  }
                }
              }
            }
          }
        ],
        "properties": {
          "@version": {
            "type": "string",
            "index": "not_analyzed"
          },
          "geoip": {
            "type": "object",
            "dynamic": true,
            "path": "full",
            "properties": {
              "location": {
                "type": "geo_point"
              }
            }
          },
          "type": {
            "type": "string",
            "index": "not_analyzed"
          },
          "host": {
            "type": "string",
            "index": "not_analyzed"
          },
          "path": {
            "type": "string",
            "index": "not_analyzed"
          },
          "thread_name": {
            "type": "string",
            "index": "not_analyzed",
            "store": "no"
          },
          "level": {
            "type": "string",
            "index": "not_analyzed",
            "store": "no"
          },
          "file": {
            "type": "string",
            "index": "not_analyzed",
            "store": "no"
          },
          "class": {
            "type": "string",
            "index": "not_analyzed",
            "store": "no"
          },
          "line_number": {
            "type": "string",
            "index": "not_analyzed",
            "store": "no"
          },
          "logger_name": {
            "type": "string",
            "index": "not_analyzed",
            "store": "no"
          },
          "method": {
            "type": "string",
            "index": "not_analyzed",
            "store": "no"
          },
          "source_host": {
            "type": "string",
            "index": "not_analyzed",
            "store": "no"
          },
          "@timestamp": {
            "format": "dateOptionalTime",
            "type": "date",
            "index": "not_analyzed"
          }
        }
      },
      "online_log": {
        "properties": {
          "message": {
            "properties": {
              "timestamp": {
                "format": "dateOptionalTime",
                "type": "date",
                "index": "not_analyzed"
              },
              "@timestamp": {
                "format": "dateOptionalTime",
                "type": "date",
                "index": "not_analyzed"
              },
              "gameCode": {
                "type": "string",
                "index": "not_analyzed"
              },
              "regionId": {
                "type": "string",
                "index": "not_analyzed"
              },
              "serverId": {
                "type": "string",
                "index": "not_analyzed"
              },
              "day_ACU": {
                "type": "long",
                "index": "not_analyzed"
              },
              "day_PCU": {
                "type": "long",
                "index": "not_analyzed"
              },
              "day_RU": {
                "type": "long",
                "index": "not_analyzed"
              },
              "day_UV": {
                "type": "long",
                "index": "not_analyzed"
              },
              "newLongin": {
                "type": "long",
                "index": "not_analyzed"
              },
              "users": {
                "type": "long",
                "index": "not_analyzed"
              },
              "uv": {
                "type": "long",
                "index": "not_analyzed"
              }
            }
          }
        }
      },
      "day_online_log": {
        "properties": {
          "message": {
            "properties": {
              "timestamp": {
                "format": "dateOptionalTime",
                "type": "date",
                "index": "not_analyzed"
              },
              "@timestamp": {
                "format": "dateOptionalTime",
                "type": "date",
                "index": "not_analyzed"
              },
              "gameCode": {
                "type": "string",
                "index": "not_analyzed"
              },
              "regionId": {
                "type": "string",
                "index": "not_analyzed"
              },
              "serverId": {
                "type": "string",
                "index": "not_analyzed"
              },
              "day_ACU": {
                "type": "long",
                "index": "not_analyzed"
              },
              "day_PCU": {
                "type": "long",
                "index": "not_analyzed"
              },
              "day_RU": {
                "type": "long",
                "index": "not_analyzed"
              },
              "day_UV": {
                "type": "long",
                "index": "not_analyzed"
              },
              "newLongin": {
                "type": "long",
                "index": "not_analyzed"
              },
              "users": {
                "type": "long",
                "index": "not_analyzed"
              },
              "uv": {
                "type": "long",
                "index": "not_analyzed"
              }
            }
          }
        }
      },
      "financial_stat": {
        "properties": {
          "message": {
            "properties": {
              "timestamp": {
                "format": "dateOptionalTime",
                "type": "date",
                "index": "not_analyzed"
              },
              "@timestamp": {
                "format": "dateOptionalTime",
                "type": "date",
                "index": "not_analyzed"
              },
              "gameCode": {
                "type": "string",
                "index": "not_analyzed"
              },
              "regionId": {
                "type": "string",
